{
  "name": "@stellarcarbon/sc-sdk",
  "version": "0.17.0",
  "description": "TypeScript SDK for the Stellarcarbon API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "npm run generate",
    "prepare": "npm run sync-version && npm run build",
    "test": "jest",
    "generate": "openapi-ts",
    "sync-version": "node scripts/sync-version.cjs"
  },
  "dependencies": {
    "axios": "^1.0.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.77.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.0.0",
    "ts-jest": "^29.4.0",
    "typescript": "^5.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stellarcarbon/sc-sdk.git"
  },
  "license": "MIT"
}
